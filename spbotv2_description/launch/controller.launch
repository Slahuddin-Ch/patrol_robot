
<launch>

<rosparam file="$(find spbotv2_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="spbotv2" args="left_wheel_joint_velocity_controller right_wheel_joint_velocity_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/spbotv2/joint_states"/>
</node>

</launch>
